{
    "name": "ResGuides: Lessons for next generation digital research tools",
    "version": "0.0.1",
    "description": "ResPlat is the department within the University of Melbourne that helps researchers with the research-specific IT things they do.",
    "repository": {
        "type": "git",
        "url": "https://github.com/resbaz/ResGuides-Gitbook"
    },
    "plugins": [
        "ga",
        "ungrey",
        "toggle-chapters",
        "mathjax",
        "sectionx",
        "youtubex",
        "edit-link",
        "atoc",
        "addcssjs"
    ],
    "pluginsConfig": {
        "ga": {
            "token": "UA-12478122-2"
        },
        "edit-link": {
            "base": "https://github.com/resbaz/ResGuides-Gitbook/edit/master",
            "label": "Edit This Page"
        },
        "addcssjs": {
            "js": ["styles/highlight.pack.js"],
            "css": ["styles/styles/default.css"]
        }
    },
    "styles": {
        "website": "styles/all.css",
        "ebook": "styles/all.css",
        "pdf": "styles/all.css",
        "mobi": "styles/all.css",
        "epub": "styles/all.css"
    },
    "links": {
        "sidebar": {
            "resbaz.edu.au": "http://melbourne.resbaz.edu.au/"
        }
    }
}